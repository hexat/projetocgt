<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="javafx.scene.layout.GridPane" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Direction Mode" GridPane.rowIndex="1" />
      <Label text="Posição inicial" GridPane.rowIndex="2" />
      <Label text="Posição final" GridPane.rowIndex="3" />
      <Label text="Inteligente" GridPane.rowIndex="4" />
      <Label text="Direction Police" />
      <ComboBox fx:id="boxModes" prefWidth="150.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
      <TextField fx:id="txtPosIniX" promptText="x" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="txtPosIniY" promptText="y" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <TextField fx:id="txtPosFimX" promptText="x" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <CheckBox fx:id="chkIntel" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <ComboBox fx:id="boxPolicies" prefWidth="150.0" GridPane.columnIndex="1" GridPane.columnSpan="2" />
      <TextField fx:id="txtPosFimY" promptText="y" GridPane.columnIndex="2" GridPane.rowIndex="3" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
