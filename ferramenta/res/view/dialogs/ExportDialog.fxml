   <right>
      <Button fx:id="btnOk" mnemonicParsing="false" text="Iniciar" BorderPane.alignment="CENTER" />
   </right>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
