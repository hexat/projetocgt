<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root prefWidth="300.0" type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" prefWidth="140.0" />
      <ColumnConstraints hgrow="SOMETIMES" prefWidth="114.0" />
      <ColumnConstraints hgrow="SOMETIMES" prefWidth="114.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label text="textureUp"  />
      <Label text="textureDown" GridPane.rowIndex="1" />
      <TextField fx:id="txtTextureUp" GridPane.columnIndex="1" />
      <TextField fx:id="txtTextureDown" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="btnTextureUp" mnemonicParsing="false" text="Procurar" GridPane.columnIndex="2" />
      <Button fx:id="btnTextureDown" mnemonicParsing="false" text="Procurar" GridPane.columnIndex="2" GridPane.rowIndex="1" />
   </children>
</fx:root>
