<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <TitledPane animated="false" expanded="false" prefWidth="200.0" text="Detalhes" BorderPane.alignment="CENTER">
         <content>
            <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </TitledPane>
   </bottom>
   <top>
      <ProgressBar prefWidth="600.0" progress="0.0" BorderPane.alignment="CENTER" />
