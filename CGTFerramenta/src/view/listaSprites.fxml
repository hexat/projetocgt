<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.ListView?>


<fx:root type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" onAction="#editSprite" text="Editar" />
            <Button mnemonicParsing="false" onAction="#delSprite" text="Deletar" />
        </items>
      </ToolBar>
      <ListView fx:id="listViewSprites" prefHeight="200.0" prefWidth="200.0" />
   </children>
</fx:root>
